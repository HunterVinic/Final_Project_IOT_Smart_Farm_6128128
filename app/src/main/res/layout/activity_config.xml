<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".Config">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:text="Config"
        tools:ignore="MissingConstraints"
        android:gravity="center"
        android:textSize="50sp"
        tools:layout_editor_absoluteX="163dp"
        tools:layout_editor_absoluteY="50dp" />

    <TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="115dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">


                <Spinner
                    android:id="@+id/spinner"
                    android:layout_width="90dp"
                    android:layout_height="50dp"
                    android:layout_weight="1"
                    android:entries ="@array/spinner1"/>



                <Button
                    android:id="@+id/search"
                    android:layout_width="wrap_content"
                    android:layout_height="64dp"
                    android:text="Search"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="213dp"
                    tools:layout_editor_absoluteY="459dp" />


            </LinearLayout>



        </TableRow>



        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.constraintlayout.widget.Barrier
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:barrierDirection="left" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/date"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Time:         "
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="34dp"
                    tools:layout_editor_absoluteY="273dp" />

                <EditText
                    android:id="@+id/min"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="00"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="100dp"
                    tools:layout_editor_absoluteY="261dp" />


                <EditText
                    android:id="@+id/sec"
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="00"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="100dp"
                    tools:layout_editor_absoluteY="261dp" />

            </LinearLayout>

        </TableRow>


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Moisture1"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="Moisture1(%)         "
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="34dp"
                    tools:layout_editor_absoluteY="273dp" />

                <EditText
                    android:id="@+id/Moisture1_data"
                    android:layout_width="270dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="00"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="100dp"
                    tools:layout_editor_absoluteY="261dp" />

            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Moisture2"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="Moisture2(%)         "
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="34dp"
                    tools:layout_editor_absoluteY="273dp" />

                <EditText
                    android:id="@+id/Moisture2_data"
                    android:layout_width="270dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="00"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="100dp"
                    tools:layout_editor_absoluteY="261dp" />

            </LinearLayout>

        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Moisture3"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="Moisture3(%)         "
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="34dp"
                    tools:layout_editor_absoluteY="273dp" />

                <EditText
                    android:id="@+id/Moisture3_data"
                    android:layout_width="270dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="00"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="100dp"
                    tools:layout_editor_absoluteY="261dp" />

            </LinearLayout>

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/NPK46"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="NPK46(cc)         "
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="34dp"
                    tools:layout_editor_absoluteY="273dp" />

                <EditText
                    android:id="@+id/NPK46_data"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="00"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="100dp"
                    tools:layout_editor_absoluteY="261dp" />
                <TextView
                    android:id="@+id/NPK15"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="NPK15(cc)         "
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="34dp"
                    tools:layout_editor_absoluteY="273dp" />

                <EditText
                    android:id="@+id/NPK15_data"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="00"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="100dp"
                    tools:layout_editor_absoluteY="261dp" />

            </LinearLayout>

        </TableRow>
        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/Temp"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="Temp(C)         "
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="34dp"
                    tools:layout_editor_absoluteY="273dp" />

                <EditText
                    android:id="@+id/Temp_data"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="00"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="100dp"
                    tools:layout_editor_absoluteY="261dp" />
                <TextView
                    android:id="@+id/FOG"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:text="FOG(sec)         "
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="34dp"
                    tools:layout_editor_absoluteY="273dp" />

                <EditText
                    android:id="@+id/FOG_data"
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:ems="10"
                    android:inputType="textPersonName"
                    android:text="00"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="100dp"
                    tools:layout_editor_absoluteY="261dp" />

            </LinearLayout>

        </TableRow>



        <TableRow
            android:layout_width="match_parent"
            android:layout_height="506dp">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="horizontal">

                <Button
                    android:id="@+id/load"
                    android:layout_width="205dp"
                    android:layout_height="63dp"
                    android:text="LOAD"
                    app:backgroundTint="#4CAF50"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="53dp"
                    tools:layout_editor_absoluteY="459dp" />

                <Button
                    android:id="@+id/back"
                    android:layout_width="205dp"
                    android:layout_height="64dp"
                    android:text="BACK"
                    app:backgroundTint="#2196F3"
                    tools:ignore="MissingConstraints"
                    tools:layout_editor_absoluteX="213dp"
                    tools:layout_editor_absoluteY="459dp" />
            </LinearLayout>
        </TableRow>>

    </TableLayout>


</LinearLayout>